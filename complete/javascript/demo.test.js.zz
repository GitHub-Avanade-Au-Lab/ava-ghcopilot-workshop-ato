const memoizedFibonacci = require("./demo");

describe("memoizedFibonacci", () => {
  it("returns the correct Fibonacci number for a given input", () => {
    expect(memoizedFibonacci(0)).toBe(0);
    expect(memoizedFibonacci(1)).toBe(1);
    expect(memoizedFibonacci(2)).toBe(1);
    expect(memoizedFibonacci(3)).toBe(2);
    expect(memoizedFibonacci(4)).toBe(3);
    expect(memoizedFibonacci(5)).toBe(5);
    expect(memoizedFibonacci(6)).toBe(8);
    expect(memoizedFibonacci(7)).toBe(13);
    expect(memoizedFibonacci(8)).toBe(21);
    // Add more test cases as needed
  });
});
